@model FanEase.UI.Models.State.StateVm

@{
    ViewData["Title"] = "StateDetails";
    Layout = "AdminLayout";
}

<body class="body-bg-f8faff">
    <div class="contact-list-area">
        <div class="container-fluid">
            <div class="table-responsive" data-simplebar>
                <div class="others-title">
                    <h3>State Details</h3>
                </div>
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StateId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StateId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StateName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StateName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CountryId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CountryId)
        </dd>
    </dl>
</div>
<div>
    <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("EditState","State",new{StateId=Model.StateId})'">Edit</button>
    <button type="button" class="btn btn-warning" onclick="location.href='@Url.Action("StateList","State",new{StateId=Model.StateId})'">Back To List</button>
</div>

            </div>
            </div>
            </div>
            </body>